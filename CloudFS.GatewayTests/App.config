<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="test" type="IgorSoft.CloudFS.GatewayTests.Config.TestSection, IgorSoft.CloudFS.GatewayTests, Version=1.0.11.0, Culture=neutral, PublicKeyToken=null"/>
  </configSections>
  <test libPath=".\Gateways;.\LegacyGateways">
    <gateways>
      <gateway schema="box" type="Async" userName="BoxUser" mount="B" exclusions="None" maxFileSize="128MB" maxPathLength="270"/>
      <!--<gateway schema="copy" type="Async" userName="CopyUser" mount="Z" exclusions="CopyDirectoryItem,CopyFileItem,MoveDirectoryItem,MoveFileItem,RenameDirectoryItem,RenameFileItem" />-->
      <gateway schema="file" type="Sync" mount="F" parameters="root=..\..\..\TestData" exclusions="ItemId"/>
      <!--<gateway schema="gcs" type="Async" userName="GoogleCloudStorageUser" mount="X" parameters="bucket=- - Insert bucket ID here - -" exclusions="CopyDirectoryItem,RenameDirectoryItem,RenameFileItem,ItemId" maxFileSize="256MB" maxPathLength="32768"
               apiKey="- - Insert Google Cloud Storage Credentials here - -" />-->
      <gateway schema="gdrive" type="Async" userName="GDriveUser" mount="G" exclusions="CopyDirectoryItem" maxFileSize="256MB" maxPathLength="32768"/>
      <gateway schema="gdrive_v2" type="Async" userName="GDriveUser" mount="X" exclusions="CopyDirectoryItem" maxFileSize="256MB" maxPathLength="30720" testDirectory="FileSystemTests-V2"/>
      <gateway schema="hubic" type="Async" userName="hubiCUser" mount="H" parameters="container=default" exclusions="CopyDirectoryItem,MoveDirectoryItem,MoveFileItem,RenameDirectoryItem,RenameFileItem,ItemId" maxFileSize="160MB" maxPathLength="512"/>
      <gateway schema="mediafire" type="Async" userName="MediaFireUser" mount="M" exclusions="ClearContent,CopyDirectoryItem" maxFileSize="12MB" maxPathLength="270"/>
      <gateway schema="mega" type="Async" userName="MegaUser" mount="N" exclusions="ClearContent,SetContent,CopyDirectoryItem,CopyFileItem" maxFileSize="256MB" maxPathLength="32768"/>
      <gateway schema="onedrive" type="Async" userName="OneDriveUser" mount="O" maxFileSize="256MB" maxPathLength="270"/>
      <gateway schema="onedrive_legacy" type="Async" userName="OneDriveUser" mount="Q" exclusions="ClearContent" maxFileSize="48MB" maxPathLength="276" testDirectory="FileSystemTests-Legacy"/>
      <gateway schema="onedrive_v1" type="Async" userName="OneDriveUser" mount="Z" maxFileSize="48MB" maxPathLength="274" testDirectory="FileSystemTests-V1"/>
      <gateway schema="pcloud" type="Async" userName="pCloudUser" mount="P" exclusions="CopyDirectoryItem" maxFileSize="16MB" maxPathLength="978"/>
      <gateway schema="webdav" type="Async" userName="WebDAVUser" mount="W" parameters="baseAddress=https://webdav.magentacloud.de" exclusions="ItemId" maxFileSize="256MB" maxPathLength="270"/>
      <gateway schema="yandex" type="Async" userName="YandexUser" mount="Y" exclusions="ItemId" maxFileSize="256MB" maxPathLength="270"/>
    </gateways>
  </test>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.2"/>
  </startup>
</configuration>
